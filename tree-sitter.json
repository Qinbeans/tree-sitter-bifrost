{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mimi",
      "camelcase": "Mimi",
      "title": "Mimi",
      "scope": "source.Mimi",
      "file-types": ["mimi"],
      "injection-regex": "^mimi$",
      "class-name": "TreeSitterMimi"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Mimi grammar for tree-sitter",
    "authors": [
      {
        "name": "Ryan Fong",
        "email": "ryan.lawrence.fong@gmail.com",
        "url": "https://qinbeans.net/"
      }
    ],
    "links": {
      "repository": "https://github.com/Qinbeans/mimi-lang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
